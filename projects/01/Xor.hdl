/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    And(a=a, b=b, out=andOut);

    Or(a=a, b=b, out=orOut);
    Not(in=orOut, out=notOut);

    Or(a=andOut, b=notOut, out=orOut2);
    Not(in=orOut2, out=out);

}